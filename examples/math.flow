// math.flow - Functions that return values

do add(a, b):
    give a + b

do multiply(a, b):
    give a * b

do square(x):
    give x * x

do factorial(n):
    if n <= 1:
        give 1
    give n * factorial(n - 1)

do main:
    say "2 + 3 ="
    say add(2, 3)

    say "4 * 5 ="
    say multiply(4, 5)

    say "7 squared ="
    say square(7)

    say "5! ="
    say factorial(5)
